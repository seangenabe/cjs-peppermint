  "author": "Sean Genabe <seangenabe@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seangenabe/cjs-peppermint/issues"
  },
  "homepage": "https://github.com/seangenabe/cjs-peppermint#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "Peppermint": "github:wilddeer/peppermint#1.4.0",
    "commonjs-exporter": "file:commonjs-exporter",
    "cpy-cli": "^1.0.1",
    "remove-jquery": "file:remove-jquery"
  }
}
