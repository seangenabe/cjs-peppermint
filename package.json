{
  "name": "@seangenabe/peppermint",
  "version": "1.0.0+1.4.0",
  "description": "CommonJS wrapper for Peppermint - touch slider",
  "main": "peppermint.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel node_modules/Peppermint/dist/peppermint.js --out-file peppermint.js && cpy node_modules/Peppermint/dist/peppermint.required.css . && cpy node_modules/Peppermint/dist/peppermint.suggested.css .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seangenabe/cjs-peppermint.git"
  },
  "keywords": [
    "slider"
